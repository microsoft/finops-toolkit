  "cSpell.words": [
    "ADLS",
    "architecting",
    "autoshutdown",
    "autostop",
    "autostopping",
    "bandersmsft",
    "CARML",
    "chargeback",
    "compat",
    "CostManagement",
    "deallocate",
    "deallocated",
    "FinOps",
    "hackathon",
    "interruptible",
    "ITAM",
    "Kusto",
    "Lookback",
    "micflan",
    "MOSA",
    "msexports",
    "PAYG",
    "PBIX",
    "Quickstart",
    "quickstarts",
    "Roadmap",
    "showback",
    "startswith",
    "templating",
    "triaging",
    "Unhide",
    "westus"
